<!DOCTYPE html>
<html>
  <head>
    <title>Bookmark Manager</title>
  </head>

  <body>
    <% if current_user %>
      Welcome, <%= current_user.email %>
    <% end %>
    <% if flash[:warning] %>
     <div class="alert alert-block">
      <%= flash[:warning] %>
     </div>
    <% end %>
    <%= yield %> 
  </body>
</html>
